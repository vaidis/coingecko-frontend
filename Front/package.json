    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/src/test/**",
      "!**/src/index.tsx"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
